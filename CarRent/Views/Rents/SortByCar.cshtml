@model IEnumerable<CarRent.Models.Rent>
@{
    ViewBag.Title = "Rent";
}

<h2>Kierowcy</h2>
<p>
    @Html.ActionLink("Create New", "Create")
    @Html.ActionLink("Show all", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomersID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Car)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Payment)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            @Html.Partial("_RentSel", item)
        </tr>
    }

</table>
